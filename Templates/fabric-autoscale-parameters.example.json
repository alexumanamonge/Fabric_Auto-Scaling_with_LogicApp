{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "logicAppName": {
      "value": "FabricAutoScaleLogicApp"
    },
    "location": {
      "value": "eastus"
    },
    "fabricCapacityName": {
      "value": "YOUR_FABRIC_CAPACITY_NAME"
    },
    "fabricWorkspaceId": {
      "value": "YOUR_FABRIC_WORKSPACE_ID"
    },
    "notificationEmail": {
      "value": "admin@yourdomain.com"
    },
    "scaleUpSku": {
      "value": "F128"
    },
    "scaleDownSku": {
      "value": "F64"
    },
    "scaleUpThreshold": {
      "value": 80
    },
    "scaleDownThreshold": {
      "value": 40
    }
  }
}
